{
  "kind": "collectionType",
  "collectionName": "amazon_products",
  "info": {
    "singularName": "amazon-product",
    "pluralName": "amazon-products",
    "displayName": "AmazonProduct",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "url": {
      "type": "string"
    },
    "categoriaProducto": {
      "type": "enumeration",
      "enum": [
        "Ámate",
        "Tu hogar",
        "Bebés",
        "Deporte y salud",
        "Ropa, bolsas y calzad y más"
      ]
    }
  }
}
